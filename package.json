{
  "name": "slidev-theme-rockdove",
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "slidev-theme",
    "slidev"
  ],
  "engines": {
    "node": ">=18.0.0",
    "slidev": ">=0.19.3"
  },
  "scripts": {
    "build": "slidev build example.md",
    "dev": "slidev example.md --open -p 3031",
    "export": "slidev export example.md",
    "screenshot": "slidev export example.md --format png"
  },
  "dependencies": {
    "@slidev/types": "^52.6.0",
    "codemirror-theme-vars": "^0.1.2",
    "prism-theme-vars": "^0.2.5",
    "@iconify-json/logos": "^1.2.9",
    "@iconify-json/solar": "^1.2.4"
  },
  "devDependencies": {
    "@slidev/cli": "^52.6.0"
  },
  "//": "Learn more: https://sli.dev/themes/write-a-theme.html",
  "slidev": {
    "colorSchema": "both",
    "highlighter": "all",
    "defaults": {
      "fonts": {
        "sans": "Atkinson Hyperlegible",
        "serif": "Zilla Slab",
        "custom": "Zilla Slab Highlight",
        "mono": "Inconsolata",
        "weights": "200,300,400,500,600"
      }
    }
  }
}